Algoritmo "Antecessor"

Var
numero, antecessor : inteiro

Inicio
escreval ("Descubra o antecessor!")
escreval("                   ")
escreval ("digite um numero")
leia(numero)
limpatela()

antecessor := numero - 1, antecessor

escreval ("O antecessor é : ", antecessor)
escreval ("Prontinho!")

Fimalgoritmo